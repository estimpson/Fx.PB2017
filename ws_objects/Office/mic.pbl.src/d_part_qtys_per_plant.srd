 procedure="1 select
	o.plant
,	o.location
,	o.std_quantity
,	o.status
,	tot_committed =
		(	select
				sum(o1.quantity)
			from
				object as o1
			where
				o1.part = o.part
				and o1.shipper > 0
		)
from
	dbo.object o
where
	o.part = :part
order by
	o.plant asc
,	o.location asc
" arguments=(("part", string)) )
