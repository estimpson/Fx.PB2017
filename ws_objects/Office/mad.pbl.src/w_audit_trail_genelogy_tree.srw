public subroutine drop_node (ref radiobutton node);
//node.x = PointerX()
//node.y = PointerY()
node.Move(PointerX(), PointerY())
node.Checked = FALSE
node.Drag(end!)
end subroutine

public subroutine draw_bom ();
//************************************************************************
//* Declaration
//************************************************************************
 
Int iRow                  //Curent row number
Int iParentX              //X coordinate for parent node
Int iParentY              //Y coordinate for parent node
Int iComponentX           //X coordinate for component node
Int iComponentY			  //Y coordinate for componet node
Int iStackStart           //The start pointer for the stack 
Int iStackEnd             //The end pointer for the stack
Int iLevelPos[20]         //The leftmost position for the each level in the tree
Int iCount                //The temporary count variable
Int iArrayCount			  //The counting varialble in the TransList array
Int iChildBeginPos		  //The beginning pos for child
Int iChildEndPos			  //The ending pos for child
Int iTotalRows				  //The total number of rows

String szParent           //The parent part  
String szRouter_code      //The router code
String szCurrentOperation //The current operation 

RadioButton rParent       //Parent node
RadioButton rComponent    //Component node

Boolean	bProcess	= TRUE

//************************************************************************
//* Initialization
//************************************************************************

iStackStart      	= 1      				//Stack always starts at 1
iStackEnd        	= 1      				//Initialize the stack ending pointer
iArrayCount			= 1

Stack[1].part 	= String(TransList[1].Serial)	//Initialize the top of stack
Stack[1].x    	= 1200
Stack[1].y    	= 100
Stack[1].pos	= 1

node[1].x      = Stack[iStackStart].x         //Assign the first node
node[1].y      = Stack[iStackStart].y         //in the tree
node[1].Text   = Stack[iStackStart].part
node[1].Tag		= "1"
node[1].Visible= TRUE

Stack[1].node	= node[1]

iNodeCounter	= 1
iLineCounter   = 1
iTreeCounter   = 1
iTotalRouterButton = 0
iLabelCounter	= 1
	
FOR iCount = 1 TO iTotalAvailableLine
	link[iCount].visible = FALSE
NEXT

For iCount = 2 To iTotalAvailableNode
	Node[iCount].visible = FALSE
NEXT

For iCount = 1 To 40
	st_label[iCount].Visible = FALSE
Next

//************************************************************************
//* Main Routine
//************************************************************************

Do while iStackStart <= iStackEnd AND bProcess

	iParentX                   = Stack[iStackStart].node.x
	iParentY                   = Stack[iStackStart].node.y
	rParent                    = Stack[iStackStart].node

	//Set the position for description
	st_label[iLabelCounter].x		= iParentX  + 40
	st_label[iLabelCounter].y		= iParentY	+ 70	
	
	If w_show_legend.ddlb_selection.text	= "Part" then
		st_label[iLabelCounter].text	= TransList[Stack[iStackStart].pos].Part
	Else
		st_label[iLabelCounter].text	= Trim(TransList[Stack[iStackStart].pos].trans_type) + ' ' + Trim(String(TransList[Stack[iStackStart].pos].Last_date)) 
	End If

	st_label[iLabelCounter].TextColor= wf_get_color(TransList[Stack[iStackStart].pos].trans_type)
	st_label[iLabelCounter].visible	= TRUE
	iLabelCounter						= iLabelCounter + 1

	wf_show_legend(TransList[Stack[iStackStart].pos].trans_type)

	
  	//Get positions for all the components		
	
	dw_stack.SetFilter("parent =" + String(Stack[iStackStart].pos))
	dw_stack.Filter()
	
	iStackStart = iStackStart + 1           			//Pop up parent from STACK

	//Set the initial X position for components
	iComponentX = iParentX - ( (dw_stack.RowCount() - 1) * 200)
	iComponentY = iParentY + 200
	
	If iLevelPos[(iComponentY - 100)/200] >= (iComponentX - 30) Then
		iComponentX = iLevelPos[(iComponentY - 100)/200] + 400
	End If

	//Push all components into stack
	//If iChildBeginPos > 0 then							//Check whether it has children

	iRow 			= 1
	iTotalRows	= dw_stack.RowCount()

	Do while (iRow <= iTotalRows) AND (iRow <= iTotalAvailableNode)			//Select all children

		iNodeCounter = iNodeCounter + 1

		node[iNodeCounter].x       = iComponentX
		node[iNodeCounter].y       = iComponentY
		node[iNodeCounter].Text    = String(TransList[dw_stack.GetItemNumber(iRow, "child")].Serial)
		node[iNodeCounter].Tag		= String(dw_stack.GetItemNumber(iRow, "child"))
		node[iNodeCounter].Visible = TRUE
		
		link[iLineCounter].BeginX  = (iParentX + 10)
		link[iLineCounter].BeginY  = (iParentY + (rParent.Height / 2))
		link[iLineCounter].EndX    = (iComponentX + 10)
		link[iLineCounter].EndY    = (iComponentY + (node[iNodeCounter].height / 2))
		link[iLineCounter].Visible = TRUE
		rComponent 						= node[iNodeCounter]

		iStackEnd 						= iStackEnd + 1
		Stack[iStackEnd].part 		= String(TransList[dw_stack.GetItemNumber(iRow, "child")].Serial)
		Stack[iStackEnd].x    		= iComponentX
		Stack[iStackEnd].y    		= iComponentY
		Stack[iStackEnd].pos			= dw_stack.GetItemNumber(iRow, "child")

		Stack[iStackEnd].node =  node[iNodeCounter]
	
		iLevelPos[(iComponentY - 100)/200] = iComponentX	
		iComponentX          = iComponentX  + 400
		iLineCounter++
		iRow ++
		
	Loop

	If iRow > iTotalAvailableNode then
		bProcess	= FALSE
	End If		
	
Loop


If Not bProcess then
		MessageBox("Warning", "System is out of resource to draw the geneology tree.", StopSign!)
End If
end subroutine

