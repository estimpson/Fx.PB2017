public subroutine wf_search_history (long iserial);//************************************************************************
//* Program Logic Description:
//* iSerialCounter: To keep how many distinct serial numbers scanned
//* All transaction history for a specified serial has been saved in the 
//* SERIALHIST[] array. DW_STACK is used to set up parent/child relation
//* while the TRANSLIST is used like a detail storage array.
//************************************************************************
//* Declaration
//***********************************************************************
Long iRow1					//to keep the row number
Long iRow					//to keep the row number
Long iPrevSerial			//to keep the previous serial number
Long iCounter				//The current pos in SerialList
   
Date dTranDate				//The date for current transaction
DAte dStartDAte			//The starting date for search
Date dEndDate				//The ending date for search

Time tTranTime				//The time for current transaction
Time tStartTime			//The starting time for search
Time tEndTime				//The ending time for search

Integer iDays				//How many days for time span

String szType				//The transaction type
String szLocation			//The location for current transaction
String szParentPart		//The parent part number
String szComponent		//The component part number
String szParentOper		//The operator for the parent part number
String szComponentOper	//The operator for the component part number
String szParentJob     	//The job number in the parent part

Boolean bProcess			//Whether to process the material issue

DataWindow dwMatIssue			//datawindow for material issue 	

//***********************************************************************
//* Initialization
//***********************************************************************

iColor					= 0
dw_stack.Modify("message.color = '" + String(iColor) + "'")
dw_stack.Reset()

iSerialCounter			= 1
iCounter					= 1
SerialList[1].Pos		= 1	
SerialList[1].Serial	= iSerial
iTransCount				= 0

dw_message.visible	= TRUE

//***********************************************************************
//* Main Routine
//***********************************************************************


Do while iCounter <= iSerialCounter

	iSerial		= SerialList[iCounter].Serial
	iParentPos	= SerialList[iCounter].pos

	//wf_show_message(iSerial, szType)

	dw_serial_transaction.Retrieve(iSerial)  //Retrieve all history for the serial#

	//Process the history of the serial first
	For iRow = 1 to dw_serial_transaction.RowCount()

		//Save the detail information

		If (iParentPos = 1) OR (iRow > 1) then
			iTransCount ++
			TransList[iTransCount].Serial		= dw_serial_transaction.GetItemNumber(iRow, "serial")
			TransList[iTransCount].Last_date	= dw_serial_transaction.GetItemDateTime(iRow, "date_stamp")
			TransList[iTransCount].Part		= dw_serial_transaction.GetItemString(iRow, "part")
			TransList[iTransCount].Trans_type= dw_serial_transaction.GetItemString(iRow, "type")
			TransList[iTransCount].To_location=dw_serial_transaction.GetItemString(iRow, "to_loc")
			TransList[iTransCount].From_location=dw_serial_transaction.GetItemString(iRow, "from_loc")
			TransList[iTransCount].Quantity	= dw_serial_transAction.GetItemNumber(iRow, "quantity")
			TransList[iTransCount].work_order= dw_serial_transaction.GetItemString(iRow, "workorder")
		End If
	
		szParentPart		= dw_serial_transaction.GetItemString(iRow, "part")
		szParentOper		= dw_serial_transaction.GetItemString(iRow, "operator")

		szType				= TransList[iTransCount].Trans_type
		iColor				= wf_get_color(szType)

		wf_show_message(TransList[iTransCount].Serial, szType)
	
		wf_show_legend(szType)
	
		If (iRow > 1) then

			dw_stack.InsertRow(1)
			dw_stack.SetItem(1, "parent", iTransCount - 1)
			dw_stack.SetItem(1, "child",  iTransCount)

			//MessageBox("Part:" + szParentPart + " Type:" + szType + " Parent:" + String(iTransCount - 1), "Child:" + String(iTransCount), StopSign!)


		End If
		
	Next

	//Search the branches of transactions 

	For iRow = 1 to dw_serial_transaction.RowCount()

		szParentPart		= dw_serial_transaction.GetItemString(iRow, "part")
		szParentOper		= dw_serial_transaction.GetItemString(iRow, "operator")
		szParentJob			= dw_serial_transaction.GetItemString(iRow, "workorder")

		szType				= dw_serial_transaction.GetItemString(iRow, "type")
		iColor				= wf_get_color(szType)

		wf_show_message(TransList[iTransCount].Serial, szType)

		wf_show_legend(szType)

		//---------------------------------------------------------------
		//Process BREAK OUT transaction
		//---------------------------------------------------------------	
		If szType = "B" then		//If this is a BREAK OUT transaction
			dw_break_out.Retrieve(String(iSerial))
			For iRow1 = 1 to dw_break_out.RowCount()
				wf_update_info(dw_break_out, iRow, iRow1)
			Next
		End If

		//---------------------------------------------------------------
		//Process JOB COMPLETION transaction
		//---------------------------------------------------------------	

		If szType = "J" then		//If this is a job completion transaction


			If (Not bVerifyJob) then //Not check by job number
				dTranDate	= Date(dw_serial_transaction.GetItemDateTime(iRow, "date_stamp"))
				tTranTime	= Time(dw_serial_transaction.GetItemDateTime(iRow, "date_stamp"))

				iDays			= Integer(w_show_legend.sle_time_span.text) / 24
				dStartDate	= RelativeDate(dTranDate, (-1) * iDays)
				dEndDate		= RelativeDate(dTranDate, iDays)
				tStartTime	= RelativeTime(tTranTime, (-1) * ((Dec(w_show_legend.sle_time_span.text) - (iDays * 24)) * 3600))
				tEndTime		= RelativeTime(tTranTime, ((Dec(w_show_legend.sle_time_span.text) - (iDays * 24)) * 3600))
				szLocation	= dw_serial_transaction.GetItemString(iRow, "from_loc")
					
				dw_material_issue.Retrieve(szLocation, &
												DateTime(dStartDate, tStartTime), &
												DateTime(dEndDate, tEndTime), "M")
				dwMatIssue	= dw_material_issue
			Else
				dw_material_issue_per_job.Retrieve(szParentJob)
				dwMatIssue	= dw_material_issue_per_job
			End If

			
			For iRow1	= 1 to dwMatIssue.RowCount()

				bProcess			= TRUE
				szComponent		= dwMatIssue.GetItemString(iRow1, "part")
				szComponentOper= dwMatIssue.GetItemString(iRow1, "operator")

				//Check the Bill of Material Constrain
				If bVerifyBOM then
					dw_bom.Retrieve(szParentPart, szComponent)
					bProcess	= (dw_bom.RowCount() > 0)
				End If

				//Check the Operator Constrain
				If bVerifyOperator then
					bProcess	= (szComponentOper	= szParentOper)
				End If

				If bProcess then					

					wf_update_info(dwMatIssue, iRow, iRow1)
					
				End If

			Next
		End If	

		//---------------------------------------------------------------
		//Process COMBINING transaction
		//---------------------------------------------------------------	

		If szType = "C" then		//If this is a combine transaction

			dTranDate	= Date(dw_serial_transaction.GetItemDateTime(iRow, "date_stamp"))
			tTranTime	= Time(dw_serial_transaction.GetItemDateTime(iRow, "date_stamp"))

			dStartDate	= RelativeDate(dTranDate, -1)
			dEndDate		= RelativeDate(dTranDate, 1)
			tStartTime	= RelativeTime(tTranTime, -1)
			tEndTime		= RelativeTime(tTranTime, 1)
			szLocation	= dw_serial_transaction.GetItemString(iRow, "to_loc")
					
			dw_combine.Retrieve( String(iSerial), "C")

			For iRow1	= 1 to dw_combine.RowCount()

				bProcess			= TRUE
				szComponentOper= dw_combine.GetItemString(iRow1, "operator")

				If dw_combine.GetItemDateTime(iRow1, "date_stamp") < DateTime(dStartDate, tStartTime) then
					bProcess	= FALSE
				End If

				If dw_combine.GetItemDateTime(iRow1, "date_stamp") > DateTime(dEndDate, tEndTime) then
					bProcess	= FALSE
				End If

				//Check the Operator Constrain
				If bProcess AND bVerifyOperator then
					bProcess	= (szComponentOper = szParentOper)
				End If

				If bProcess then					

					wf_update_info(dw_combine, iRow, iRow1)

				End If

			Next

		End If									
			
	Next

iCounter ++
		
Loop

dw_message.visible	= FALSE	


end subroutine

public subroutine wf_show_message (long iserial, string sztype);dw_message.InsertRow(1)
dw_message.SetItem(1, "message", "Type:" + szType + "   Serial:" + String(iSerial))
dw_message.SetItem(1, "flag", wf_get_color(szType))
end subroutine

public subroutine wf_update_info (datawindow dwin, long iparentrow, long icurrentrow);String szType

iTransCount ++
dw_stack.InsertRow(1)
dw_stack.SetItem(1, "parent", iParentPos + iParentRow - 1)
dw_stack.SetItem(1, "child",  iTransCount)

//Get the lastest transaction for the serial number
dw_serial_transaction2.Retrieve(dwin.GetItemNumber(iCurrentRow, "serial"))

TransList[iTransCount].Serial			= dw_serial_transaction2.GetItemNumber(1, "serial")
TransList[iTransCount].Last_date		= dw_serial_transaction2.GetItemDateTime(1, "date_stamp")
TransList[iTransCount].Part			= dw_serial_transaction2.GetItemString(1, "part")
TransList[iTransCount].Trans_type	= dw_serial_transaction2.GetItemString(1, "type")
TransList[iParentPos + iParentRow - 1].Child_end_pos	= iTransCount
TransList[iTransCount].Quantity		= dw_serial_transaction2.GetItemNumber(1, "quantity")
TransList[iTransCount].To_location	= dw_serial_transaction2.GetItemString(1, "to_loc")

iSerialCounter								= iSerialCounter + 1
SerialList[iSerialCounter].Serial	= dwin.GetItemNumber(iCurrentRow, "serial")
SerialList[iSerialCounter].Pos		= iTransCount

szType	= TransList[iTransCount].Trans_type
iColor	= wf_get_color(szType)

wf_show_message(TransList[iTransCount].Serial, szType)

end subroutine

