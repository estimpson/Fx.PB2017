	shipper.id
,	shipper.date_stamp
,	shipper.shipping_dock
,	shipper.ship_via
,	shipper.ExpediteCode
,	shipper.status
,	shipper.model_year
,	shipper.plant
,	shipper.notes
,	shipper.printed
from
	shipper
where
	shipper.destination = :szDestination
	and shipper.status in ('O', 'S')
