 retrieve="
select
	LiftCount = count(distinct coalesce(oS.parent_serial, os.serial))
,	PerLiftLabels = 1
,	UseFixedLabelCount= 1
,	FixedLabelCount = convert(int, null)
,	Changed = convert(varchar(1000), null)
,	IsSelected = 0
from
	dbo.shipper s
	join dbo.object oS
		on oS.shipper = s.id
		and oS.type is null
where
	s.id = :Shipper
" arguments=(("Shipper", number)) )
