 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"po_detail~" )  TABLE(NAME=~"po_header~" )  TABLE(NAME=~"part_vendor~" ) COLUMN(NAME=~"po_header.po_number~") COLUMN(NAME=~"po_detail.part_number~") COLUMN(NAME=~"part_vendor.vendor~") COLUMN(NAME=~"po_detail.date_due~") COLUMN(NAME=~"po_detail.standard_qty~") COMPUTE(NAME=~"part_vendor.vendor + '/' + convert(varchar,po_header.po_number) as vendor_po~")    JOIN (LEFT=~"po_detail.po_number~"    OP =~"=~"RIGHT=~"po_header.po_number~" )    JOIN (LEFT=~"po_header.vendor_code~"    OP =~"=~"RIGHT=~"part_vendor.vendor~" )WHERE(    EXP1 =~"( ( part_vendor.part~"   OP =~"=~"    EXP2 =~":as_part )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( po_header.status~"   OP =~"<>~"    EXP2 =~"'C' ) )~" ) ) ARG(NAME = ~"as_part~" TYPE = string) " filter="isnull(part_number) or part_number = as_part" arguments=(("as_part", string))  sort="vendor_po A date_due A " )
