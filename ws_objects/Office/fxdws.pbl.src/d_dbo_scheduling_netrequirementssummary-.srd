 retrieve="select
	snrs.PrimaryMachineCode
,	snrs.BuildPartCode
,	snrs.OrderNo
,	snrs.BillToCode
,	snrs.ShipToCode
,	snrs.LowLevel
,	snrs.DueDT
,	snrs.DaysOnHand
,	snrs.QtyTotalDue
,	snrs.QtyAvailable
,	snrs.QtyAlreadyProduced
,	snrs.QtyNetDue
,	snrs.QtyBuildable
,	snrs.QtyCurrentBuild
,	snrs.QtyNextBuild
,	snrs.BoxLabel
,	snrs.PalletLabel
,	snrs.PackageType
,	snrs.TopPartCode
,	Change = convert(char(1000), '')
,	IsSelected = 0
from
	dbo.Scheduling_NetRequirementsSummary snrs
	left join dbo.Scheduling_InLineProcess silp
		on silp.TopPartCode = snrs.TopPartCode
		and silp.OutputPartCode = snrs.BuildPartCode
		and silp.MachineCode = snrs.PrimaryMachineCode
where
	silp.DefaultOutputPartCode = snrs.BuildPartCode
" filter=" primarymachinecode in ('3','4')" sort="primarymachinecode A duedt A " )
