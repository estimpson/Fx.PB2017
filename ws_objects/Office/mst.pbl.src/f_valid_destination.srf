global function boolean f_valid_destination (string szdestination);String szResult

SetNull(szResult)

  SELECT destination.destination  
    INTO :szResult  
    FROM destination  
   WHERE destination.destination = :szDestination   ;

Return (Not IsNull(szResult))
end function

