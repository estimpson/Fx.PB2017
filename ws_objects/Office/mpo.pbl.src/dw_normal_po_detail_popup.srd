 retrieve="select
	pd.po_number
,	pd.vendor_code
,	pd.part_number
,	pd.description
,	pd.unit_of_measure
,	pd.date_due
,	pd.notes
,	pd.quantity
,	pd.received
,	pd.balance
,	pd.alternate_price
,	pd.row_id
,	pd.release_no
,	pd.week_no
,	pd.ship_to_destination
,	pd.terms
,	pd.plant
,	pd.standard_qty
,	pd.ship_type
,	pd.printed
,	pd.deleted
,	pd.status
,	pd.sales_order
,	pd.dropship_oe_row_id
,	pd.dropship_shipper
,	pd.price_unit
,	pd.ship_via
,	pd.cross_reference_part
,	pd.account_code
,	pd.taxable
,	pd.dimension_qty_string
,	pd.scheduled_time
,	pd.job_cost_no
,	pd.requisition_id
,	rd.deliver_to_operator
,	rh.requisitioner
,	pd.promise_date
,	pd.other_charge
from
	dbo.po_detail pd
	left join dbo.requisition_detail rd
		on rd.po_number = pd.po_number
		and rd.po_rowid = pd.row_id
	left join dbo.requisition_header rh
		on rh.requisition_number = rd.requisition_number
where
	pd.po_number = :PONumber
	and pd.part_number = :Part
	and pd.row_id = :RowID
" update="po_detail" updatewhere=0 updatekeyinplace=no arguments=(("PONumber", number),("Part", string),("RowID", number)) )
