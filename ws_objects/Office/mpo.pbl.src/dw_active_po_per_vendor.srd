 retrieve="  SELECT po_header.po_number,   
         po_header.plant,   
         po_header.ship_to_destination,   
         po_header.ship_type,   
         po_header.release_control,   
         po_header.type  
    FROM po_header  
   WHERE ( po_header.vendor_code = :szVendor ) AND  
         ( po_header.status = 'A' )    
