 retrieve="SELECT shipper_detail.part, shipper_detail.qty_packed, shipper_detail.accum_shipped, shipper_detail.order_no, shipper_detail.release_no, shipper_detail.price, shipper_detail.shipper, shipper.freight, shipper.tax_percentage FROM shipper_detail, shipper WHERE ( shipper.id = shipper_detail.shipper ) and ( ( shipper_detail.shipper = :iShipper ) ) " arguments=(("iShipper", number)) )
