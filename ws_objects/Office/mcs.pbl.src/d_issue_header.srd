 retrieve="  SELECT issues.issue_number,   
         issues.issue,   
         issues.status,   
         issues.solution,   
         issues.start_date,   
         issues.stop_date,   
         issues.category,   
         issues.sub_category,   
         issues.priority_level,   
         issues.product_line,   
         issues.product_code,   
         issues.origin_type,   
         issues.origin,   
         issues.assigned_to,   
         issues.authorized_by,   
         issues.documentation_change,   
         issues.environment,
			issues.entered_by,
			issues.product_component,
			'' as environment_message 
    FROM issues  
   WHERE issues.issue_number = :al_issue    
" update="issues" updatewhere=0 updatekeyinplace=yes arguments=(("al_issue", number)) )
