 retrieve="select	
	destination.destination,
	destination.name,
	destination.address_1,
	destination.address_2,
	destination.address_3,
	destination.address_4,
	destination.address_5,
	destination.address_6,
	carrier.name,
	carrier.scac,
	shipper.id,
	shipper.net_weight,
	destination.name,
	bill_of_lading.bol_number,
	destination_shipping.note_for_bol,
	shipper.tare_weight,
	shipper.net_weight,
	shipper.staged_pallets,
	shipper.staged_objs,
	parameters.company_name,
	parameters.address_1,
	parameters.address_2,
	parameters.address_3,
	parameters.phone_number,
	shipper.shipping_dock,
	shipper.staged_pallets,
	shipper.gross_weight,
	shipper.freight_type,
	es.supplier_code,
	d2.destination,
	d2.name
from	bill_of_lading
	join shipper on bill_of_lading.bol_number = shipper.bill_of_lading_number
	join carrier on bill_of_lading.scac_transfer = carrier.scac
	join destination on bill_of_lading.destination = destination.destination
	join destination d2 on shipper.destination = d2.destination
	join destination_shipping on shipper.destination = destination_shipping.destination
	join edi_setups es on es.destination = shipper.destination
	cross join parameters
where	bill_of_lading.bol_number = convert(int,:ibillofladingnumber)
