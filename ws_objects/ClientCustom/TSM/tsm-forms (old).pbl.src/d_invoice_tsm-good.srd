 retrieve="SELECT 	shipper.id,   
         shipper.date_stamp,   
         shipper.shipping_dock,   
         shipper.ship_via,   
         shipper.date_shipped,   
         shipper.aetc_number,   
         shipper.bill_of_lading_number,   
         shipper.customer,   
         shipper_detail.part,   
         shipper_detail.qty_packed,   
         order_header.customer_part,   
         customer.customer,   
         customer.name,   
         customer.address_1,   
         customer.address_3,   
         customer.address_2,   
         edi_setups.supplier_code,   
         destination.name,   
         destination.address_1,   
         destination.address_2,   
         destination.address_3,   
         shipper.notes,   
         shipper_detail.note,   
         shipper.invoice_number,   
         customer.terms,   
         shipper_detail.price,   
         shipper.destination,   
         shipper.status,   
         order_header.our_cum,   
         destination.destination,   
         destination_shipping.fob,   
         shipper_detail.type,   
         part.part,   
         shipper_detail.alternative_unit,   
         shipper.freight_type,   
         part.name,   
         shipper.terms,   
         shipper_detail.order_no,   
         shipper_detail.suffix,   
         shipper_detail.part_original,   
         order_header.dock_code,   
         carrier.name,   
         carrier.scac,   
         carrier.trans_mode,   
         destination_shipping.scac_code,   
         destination_shipping.trans_mode,   
         shipper_detail.part_name,   
         order_header.salesman,   
         shipper_detail.taxable,   
         shipper.tax_rate,   
         shipper.freight,   
         shipper.tax_percentage,   
         order_header.customer_po,
			shipper.date_shipped  
    FROM order_header,   
         shipper,   
         shipper_detail,   
         customer,   
         destination,   
         edi_setups,   
         part,   
         destination_shipping,   
         carrier  
   WHERE ( edi_setups.destination  =* shipper.destination) and  
         ( destination_shipping.destination = shipper.destination ) and  
         ( carrier.scac = shipper.ship_via ) and  
         ( shipper.id = shipper_detail.shipper ) and  
         ( order_header.order_no = shipper_detail.order_no ) and  
         ( customer.customer = destination.customer ) and  
         ( shipper.destination = destination.destination ) and  
         ( shipper_detail.part_original = part.part ) and  
         ( ( shipper.invoice_number = ( convert (int, :linvoice ) ) ) )



