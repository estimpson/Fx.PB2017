 retrieve="  SELECT  downtime.trans_date ,           downtime.trans_time ,           downtime.machine ,           downtime.job ,           downtime.part ,           downtime.reason_code ,           downtime.reason_name ,           downtime.down_time ,           downtime.employee ,           downtime.data_source     
        FROM downtime      
        WHERE ( downtime.trans_date between :Enter_From_Date and :Enter_To_Date ) and          ( downtime.machine like :Enter_Machine_Number ) and          ( downtime.reason_code like :Enter_Reason_Code )   " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime),("Enter_Machine_Number", string),("Enter_Reason_Code", string))  sort="downtime_machine A downtime_trans_date A downtime_part A downtime_reason_code A downtime_employee A " )
