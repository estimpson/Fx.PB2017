 retrieve="  SELECT  object.serial ,           object.part ,           part.name ,           object.quantity ,           object.unit_measure ,           object.status ,           object.parent_serial ,           object.location ,           object.lot ,           object.last_date ,           object.operator ,           part_inventory.abc     
        FROM object ,           part ,           part_inventory     
        WHERE ( object.part = part.part ) and          ( object.part = part_inventory.part ) and          ( ( object.location <> 'LOST' ) ) 
        GROUP BY object.part ,           object.serial ,           part.name ,           object.quantity ,           object.custom5 ,           object.custom4 ,           object.custom3 ,           object.lot ,           object.location ,           object.parent_serial ,           object.unit_measure ,           object.operator ,           object.status ,           object.last_date ,           part_inventory.abc  
        ORDER BY object.part          ASC,           object.serial          ASC,           object.lot          ASC,           object.location          ASC,           object.parent_serial          ASC,           object.status          ASC,           object.unit_measure          ASC,           part.name          ASC,           object.quantity          ASC,           object.operator          ASC,           object.last_date          ASC  "  sort="part_inventory_abc A part A name A serial A quantity A unit_measure A status A parent_serial A location A lot A " )
