 retrieve="  SELECT  master_prod_sched.type ,           master_prod_sched.part ,           part.name ,           master_prod_sched.origin ,           master_prod_sched.source ,           master_prod_sched.due ,           master_prod_sched.dead_start ,           master_prod_sched.qnty ,           master_prod_sched.qty_assigned ,           master_prod_sched.po_number ,           master_prod_sched.workorder     
        FROM master_prod_sched ,           part     
        WHERE ( master_prod_sched.part  *= part.part) and          ( ( master_prod_sched.due between :Enter_From_Date and :Enter_To_Date ) )  " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime))  sort="master_prod_sched_part A master_prod_sched_due A " )
