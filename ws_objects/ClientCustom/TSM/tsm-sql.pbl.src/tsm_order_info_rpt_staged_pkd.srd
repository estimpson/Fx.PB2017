 retrieve="  SELECT  shipper.date_stamp ,           shipper.id ,           shipper.customer ,           shipper.destination ,           shipper_detail.part_original ,           shipper_detail.qty_required ,           shipper_detail.qty_packed ,           part_machine.machine ,           part_machine.sequence ,           shipper_detail.pack_line_qty     
        FROM part_machine ,           shipper ,           shipper_detail     
        WHERE ( shipper_detail.part_original  *= part_machine.part) and          ( shipper.id = shipper_detail.shipper ) and          ( ( shipper.date_stamp between :Enter_From_Date and :Enter_To_Date ) )  " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime))  sort="shipper_date_stamp A shipper_customer A shipper_destination A shipper_id A " )
