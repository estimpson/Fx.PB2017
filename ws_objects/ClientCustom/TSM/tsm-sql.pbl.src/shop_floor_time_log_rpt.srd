 retrieve="  SELECT  shop_floor_time_log.transaction_date_time ,           shop_floor_time_log.log_date ,           shop_floor_time_log.work_order ,           shop_floor_time_log.operator ,           shop_floor_time_log.qty ,           shop_floor_time_log.labor_hours ,           shop_floor_time_log.status ,           shop_floor_time_log.part     
        FROM shop_floor_time_log      
        WHERE ( shop_floor_time_log.transaction_date_time between :Enter_From_DateTime and :Enter_To_DateTime ) and          ( shop_floor_time_log.operator like :Enter_Operator_Initials )   " arguments=(("Enter_From_DateTime", datetime),("Enter_To_DateTime", datetime),("Enter_Operator_Initials", string))  sort="work_order A transaction_date_time A " )
