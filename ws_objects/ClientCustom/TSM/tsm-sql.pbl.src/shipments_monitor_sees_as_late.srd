 column=(type=char(10) updatewhereclause=yes name=bill_of_lading_number dbname="bill_of_lading_number" initial="spaces" )
 column=(type=long updatewhereclause=yes name=invoice_number dbname="invoice_number" )
 column=(type=char(254) updatewhereclause=yes name=notes dbname="notes" )
 column=(type=char(1) updatewhereclause=yes name=responsibility_code dbname="responsibility_code" )
 column=(type=decimal(6) updatewhereclause=yes name=freight dbname="freight" )
 retrieve="  SELECT  shipper.date_shipped ,           shipper.id ,           shipper.date_stamp ,           shipper.customer ,           shipper.destination ,           shipper.bill_of_lading_number ,           shipper.invoice_number ,           shipper.notes ,           shipper.responsibility_code ,           shipper.freight     
        FROM shipper      
        WHERE ( shipper.date_shipped > ~"shipper~".~"date_stamp~" ) and          ( shipper.time_shipped between :Enter_From_Date and :Enter_To_Date ) and          ( shipper.customer > '0' )   " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime))  sort="date_shipped A id A " )
