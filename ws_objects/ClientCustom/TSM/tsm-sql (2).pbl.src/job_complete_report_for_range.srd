 retrieve="  SELECT  audit_trail.date_stamp ,           audit_trail.serial ,           audit_trail.type ,           audit_trail.part ,           audit_trail.quantity ,           audit_trail.operator ,           audit_trail.to_loc ,           audit_trail.from_loc ,           audit_trail.status ,           audit_trail.workorder     
        FROM audit_trail      
        WHERE ( audit_trail.date_stamp between :Enter_From_DateTime and :Enter_To_DateTime ) and          ( audit_trail.type = 'J' )   " filter=" part ='CH6379-99'" arguments=(("Enter_From_DateTime", datetime),("Enter_To_DateTime", datetime))  sort="part A date_stamp A " )
