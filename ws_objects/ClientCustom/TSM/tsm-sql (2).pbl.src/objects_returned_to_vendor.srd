 retrieve="  SELECT  audit_trail.date_stamp ,           audit_trail.serial ,           audit_trail.type ,           audit_trail.part ,           audit_trail.quantity ,           audit_trail.remarks ,           audit_trail.po_number ,           audit_trail.operator ,           audit_trail.lot ,           audit_trail.shipper ,           audit_trail.status ,           audit_trail.custom5 ,           audit_trail.notes ,           audit_trail.user_defined_status     
        FROM audit_trail      
        WHERE ( audit_trail.type = 'V' ) and          ( audit_trail.date_stamp between :Enter_From_Date and :Enter_To_Date ) and          ( audit_trail.part like :Enter_Part_Number )   " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime),("Enter_Part_Number", string))  sort="part A " )
