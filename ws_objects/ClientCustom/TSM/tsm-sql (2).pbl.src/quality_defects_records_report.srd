 retrieve="  SELECT  defects.defect_date ,           defects.defect_time ,           defects.work_order ,           defects.part ,           defects.machine ,           defects.reason ,           defect_codes.name ,           defects.quantity ,           defects.operator ,           employee.name     
        FROM defect_codes ,           defects ,           employee     
        WHERE ( defects.operator = employee.operator_code ) and          ( defects.reason = defect_codes.code ) and          ( ( defects.defect_date between :Enter_From_Date and :Enter_To_Date ) and          ( defects.machine like :Enter_Machine_Number ) and          ( defects.reason like :Enter_Reason_Code ) )  " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime),("Enter_Machine_Number", string),("Enter_Reason_Code", string))  sort="defects_part A defects_defect_date A defects_machine A defects_operator A defects_reason A " )
