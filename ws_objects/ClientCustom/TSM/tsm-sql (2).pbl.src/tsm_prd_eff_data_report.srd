 retrieve="  SELECT  dbo.shop_floor_time_log.log_date ,           dbo.shop_floor_time_log.shift ,           dbo.shop_floor_time_log.operator ,           dbo.shop_floor_time_log.activity ,           dbo.shop_floor_time_log.location ,           dbo.shop_floor_time_log.part ,           dbo.shop_floor_time_log.qty ,           dbo.shop_floor_time_log.labor_hours ,           dbo.tsm_part_mach_pcs_per_hr.parts_per_hour ,           dbo.employee.name     
        FROM dbo.employee ,           dbo.shop_floor_time_log ,           dbo.tsm_part_mach_pcs_per_hr     
        WHERE ( dbo.shop_floor_time_log.part = dbo.tsm_part_mach_pcs_per_hr.part ) and          ( dbo.shop_floor_time_log.location = dbo.tsm_part_mach_pcs_per_hr.machine_no ) and          ( dbo.shop_floor_time_log.operator = dbo.employee.operator_code ) and          ( ( dbo.shop_floor_time_log.log_date between :Enter_From_Log_Date and :Enter_To_Log_Date ) and         ( dbo.employee.operator_code like :Enter_Operator_Code ) )  " arguments=(("Enter_From_Log_Date", datetime),("Enter_To_Log_Date", datetime),("Enter_Operator_Code", string))  sort="shop_floor_time_log_log_date A shop_floor_time_log_operator A " )
