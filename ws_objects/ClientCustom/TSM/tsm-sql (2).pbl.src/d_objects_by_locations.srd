table(column=(type=char(10) updatewhereclause=yes name=location dbname="location" criteria.dialog=yes)
 column=(type=long updatewhereclause=yes name=serial dbname="serial" )
 column=(type=char(25) updatewhereclause=yes name=part dbname="part" criteria.dialog=yes)
 column=(type=char(50) updatewhereclause=yes name=name dbname="name" )
 column=(type=datetime updatewhereclause=yes name=last_date dbname="last_date" initial="today" )
 column=(type=decimal(6) updatewhereclause=yes name=quantity dbname="quantity" )
 column=(type=char(2) updatewhereclause=yes name=unit_measure dbname="unit_measure" )
 column=(type=char(1) updatewhereclause=yes name=show_on_shipper dbname="show_on_shipper" )
 column=(type=decimal(0) updatewhereclause=yes name=object_parent_serial dbname="parent_serial" )
 retrieve="  SELECT  object.location ,           object.serial ,           object.part ,           part.name ,           object.last_date ,           object.quantity ,           object.unit_measure ,           object.show_on_shipper ,           object.parent_serial     
        FROM object ,           part     
        WHERE ( object.part = part.part )  
        GROUP BY object.location ,           object.part ,           object.show_on_shipper ,           object.quantity ,           object.unit_measure ,           object.last_date ,           object.serial ,           part.name ,           object.parent_serial  
        ORDER BY object.location          ASC  "  sort="location A part A serial A " )
