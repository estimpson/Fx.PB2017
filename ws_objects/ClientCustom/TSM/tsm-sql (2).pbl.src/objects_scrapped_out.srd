 retrieve="  SELECT  audit_trail.date_stamp ,           audit_trail.serial ,           audit_trail.type ,           audit_trail.part ,           audit_trail.quantity ,           audit_trail.operator ,           audit_trail.from_loc ,           audit_trail.to_loc ,           audit_trail.lot ,           audit_trail.status ,           audit_trail.user_defined_status ,           audit_trail.notes     
        FROM audit_trail      
        WHERE ( audit_trail.type = 'Q' ) and          ( audit_trail.to_loc = 'S' ) and          ( audit_trail.date_stamp between :Enter_From_Date and :Enter_To_Date )   " arguments=(("Enter_From_Date", datetime),("Enter_To_Date", datetime))  sort="part A date_stamp A " )
