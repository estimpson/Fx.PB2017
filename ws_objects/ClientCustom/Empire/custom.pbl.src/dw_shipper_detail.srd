 retrieve="  SELECT sd.part,   
         sd.qty_required,   
         sd.boxes_staged,   
         sd.gross_weight,   
         sd.shipper,   
         sd.accum_shipped,   
         sd.operator,   
         sd.date_shipped,   
         sd.order_no,   
         sd.customer_po,   
         sd.note,   
         sd.alternative_qty,   
         sd.alternative_unit,   
         sd.week_no,   
         sd.qty_packed,   
         sd.net_weight,   
         sd.tare_weight,   
         sd.suffix,   
         sd.part_name,   
         sd.pack_line_qty,   
         sd.stage_using_weight,   
         sd.customer_part,   
         sd.release_date,   
         sd.release_no,
			pi.configurable,
			sd.part_original,
	 pol.non_ship,
 	 pol.non_ship_note,
	 pol.non_ship_operator,
	 sd.group_no
    FROM shipper_detail sd 
			join part_inventory pi on pi.part = sd.part_original 
	 join part_eecustom pol on pol.part = pi.part
   WHERE sd.shipper = :iShipper    
" update="shipper_detail" updatewhere=0 updatekeyinplace=yes arguments=(("iShipper", number))  sort="part A " )
