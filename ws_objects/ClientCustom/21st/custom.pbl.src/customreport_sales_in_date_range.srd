 column=(type=char(35) updatewhereclause=yes name=part dbname="shipper_detail.part" criteria.dialog=yes)
 column=(type=decimal(6) updatewhereclause=yes name=qty_packed dbname="shipper_detail.qty_packed" )
 column=(type=decimal(6) updatewhereclause=yes name=shipper_detail_price dbname="shipper_detail.price" )
 column=(type=long updatewhereclause=yes name=shipper_invoice_number dbname="shipper.invoice_number" )
 column=(type=char(25) updatewhereclause=yes name=shipper_detail_customer_po dbname="shipper_detail.customer_po" )
 retrieve="select	shipper.date_shipped,
	shipper.destination,
	shipper_detail.part,
	shipper_detail.qty_packed,
	shipper_detail.price,
	shipper.invoice_number,
	shipper_detail.customer_po
from	shipper
	join shipper_detail on shipper.id = shipper_detail.shipper
	left join order_header on shipper_detail.order_no = order_header.order_no
where	shipper.date_shipped >= :fromdate and shipper.date_shipped < :todate + 1
" arguments=(("FROMDATE", datetime),("TODATE", datetime))  sort="part A date_shipped A shipper_destination A " )
