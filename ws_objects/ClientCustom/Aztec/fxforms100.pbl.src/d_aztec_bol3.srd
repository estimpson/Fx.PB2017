 retrieve="SELECT destination.address_1, 
		 destination.address_2,
 		 destination.address_3, 
		 carrier.name, 
		 carrier.scac, 
		 shipper.id, 
       	bill_of_lading.scac_transfer,
		 shipper.net_weight, 
		 destination.name, 
		 bill_of_lading.bol_number, 
		 destination_shipping.note_for_bol, 
		 shipper.tare_weight, 
		 shipper.net_weight,
		 shipper.staged_pallets,
		 shipper.staged_objs, 
		 parameters.company_name, 
		 parameters.address_1, 
		 parameters.address_2, 
		 parameters.address_3,
		 parameters.phone_number,
		 shipper.destination,
		 shipper.gross_weight,
		 edi_setups.parent_destination,
       edi_setups.material_issuer,
       shipper.bill_of_lading_number,
       bill_of_lading.trans_mode,
       DATEPART(dy, getdate()) as day_of_year,
       shipper.shipping_dock,
       edi_setups.supplier_code,
	(Select min(customer_po) from shipper_detail where shipper =shipper.id) as ReleaseNo
FROM 	 carrier, 
		 destination, 
	    shipper, 
		 bill_of_lading, 
	    destination_shipping, 
	    parameters,
       edi_setups
WHERE  ( shipper.destination = destination_shipping.destination ) and 
		 ( bill_of_lading.destination = destination.destination ) and 	
		 ( bill_of_lading.scac_transfer = carrier.scac ) and 
		 (shipper.bill_of_lading_number = convert(int,:iBillOfLadingNumber )) and
		 (shipper.bill_of_lading_number = bill_of_lading.bol_number) and
       	( bill_of_lading.destination = edi_setups.destination )

" arguments=(("iBillofLadingNumber", string)) )
