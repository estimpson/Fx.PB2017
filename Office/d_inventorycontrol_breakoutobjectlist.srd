 retrieve="select
	ico.Serial
,	ico.Part
,	ico.StdQuantity
,	ico.StdUnit
,	ico.Location
,	ico.Plant
,	ico.Status
,	ico.UserDefinedStatus
,	ico.PackageType
,	ico.PalletSerial
,	ico.Lot
,	ico.LicensePlate
,	Changed = convert(varchar(1000), '')
,	IsSelected = 0
from
	dbo.InventoryControl_Objects ico
where
	ico.Serial in (:SerialList)
	and ico.StdQuantity >= dbo.udf_GetStdQtyFromQty(ico.Part, :qtyPerObject * :objectCount, :unit)
" arguments=(("serialList", numberlist),("qtyPerObject", number),("unit", string),("objectCount", number)) )
